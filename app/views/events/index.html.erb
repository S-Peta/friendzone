</div>

<div class="container">
  <% @events.each do |event| %>
    <div class="card">
      <%= link_to event_path(event) do %>
        <h2><%= event.name %></h2>
        <p><%= event.event_date %></p>
        <p><%= event.location %></p>
      <% end %>
    </div>
  <% end %>
</div>
